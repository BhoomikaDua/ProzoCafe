<div class="prozo-invoices-wrapper">
  <h1 class="user-form-title">Order History</h1>
  <% @invoices.pendingOrders.map do |invoice|%>
    <%= render partial: "invoices-listing", locals: {
      invoice: invoice
    } %>
  <% end %>

  <% @invoices.deliveredOrders.map do |invoice|%>
    <%= render partial: "invoices-listing", locals: {
      invoice: invoice
    } %>
  <% end %>

</div>
